{
  "name": "itw-test-app",
  "private": true,
  "version": "0.0.0",
  "description": "Test App",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "node dist/main",
    "lint": "eslint --ext ts,js",
    "migration:create": "./bin/run-ts-node src/database/migrator.ts create",
    "migration:down": "./bin/run-ts-node src/database/migrator.ts down",
    "migration:up": "./bin/run-ts-node src/database/migrator.ts up",
    "seeds:up": "./bin/run-ts-node src/database/seeder.ts",
    "seeds:fresh": "./bin/run-ts-node src/database/seeder.ts --truncate",
    "prettier:check": "prettier --check '**/*.{json,md,yml,yaml,html,css}' --ignore-path .gitignore",
    "run-ts-node": "./bin/run-ts-node",
    "test": "jest",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@mikro-orm/core": "4.5.9",
    "@mikro-orm/migrations": "4.5.9",
    "@mikro-orm/nestjs": "4.3.0",
    "@mikro-orm/postgresql": "4.5.9",
    "@mikro-orm/seeder": "5.0.0-dev.112",
    "@nestjs/common": "8.0.6",
    "@nestjs/core": "8.0.6",
    "@nestjs/platform-express": "8.0.6",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "envalid": "7.2.1",
    "faker": "5.5.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.3.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.1",
    "@nestjs/schematics": "8.0.2",
    "@nestjs/testing": "8.0.6",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.1",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.1",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "supertest": "6.1.6",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.1",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
