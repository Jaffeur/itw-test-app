{
  "name": "itw-test-app-frontend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^14.16"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production && next build",
    "build:styles": "tailwind build src/assets/css/tailwind.css -o public/css/styles.min.css",
    "dev": "yarn run dev:server",
    "dev:server": "npx next dev -p ${PORT:-3001}",
    "prettier:check": "prettier --check '**/**/*.{json,md,yml,yaml,css,ts,tsx,js,jsx}'",
    "start": "cross-env NODE_ENV=production node server.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "0.21.1",
    "clsx": "1.1.1",
    "dotenv": "8.2.0",
    "envalid": "7.1.0",
    "express": "4.17.1",
    "formik": "2.2.6",
    "helmet": "4.5.0",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.3.1",
    "react-query": "3.26.0",
    "tailwindcss": "1.9.5",
    "type-fest": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.16",
    "@types/node": "14.14.41",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "babel-plugin-module-resolver": "4.1.0",
    "cross-env": "7.0.3",
    "lint-staged": "10.5.4",
    "prettier": "2.3.2",
    "prettier-plugin-package": "1.3.0",
    "typescript": "4.3.5",
    "webpack": "5.35.0"
  }
}
